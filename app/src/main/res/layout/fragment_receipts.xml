<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

  <data>
  <variable
    name="receiptsViewModel"
    type="com.sakura.easyrent.fragments.receiptsfragment.ReceiptsItem" />
  </data>

  <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      tools:context=".fragments.receiptsfragment.ReceiptsFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/RecycleView_Receipts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:listitem="@layout/receipts_item"/>
  </androidx.constraintlayout.widget.ConstraintLayout>
</layout>